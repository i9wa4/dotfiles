{
    "@jupyterlab/apputils-extension:themes": {
        "theme": "JupyterLab Dark"
    },
    "@jupyterlab/fileeditor-extension:plugin": {
        "editorConfig": {
            "fontFamily": "'MyricaM M', monospace",
            "fontSize": 18,
            "lineNumbers": true,
            "lineWrap": "on",
            "rulers": [
                80
            ],
            "showTrailingSpace": true
        }
    },
    "@jupyterlab/notebook-extension:tracker": {
        "codeCellConfig": {
            "fontFamily": "'MyricaM M', monospace",
            "fontSize": 18,
            "lineWrap": "on",
            "rulers": [
                80
            ],
            "showTrailingSpace": true
        },
        "defaultCell": "code",
        "markdownCellConfig": {
            "fontFamily": "'MyricaM M', monospace",
            "fontSize": 18,
            "lineWrap": "on",
            "rulers": [
                80
            ],
            "showTrailingSpace": true
        },
        "rawCellConfig": {
            "fontFamily": "'MyricaM M', monospace",
            "fontSize": 18,
            "lineWrap": "on",
            "rulers": [
                80
            ],
            "showTrailingSpace": true
        }
    }
}
