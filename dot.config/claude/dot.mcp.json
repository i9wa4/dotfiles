{
    "mcpServers": {
        "atlassian": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.atlassian.com/v1/sse"
            ],
            "env": {},
            "disabled": true,
            "autoApprove": []
        },
        "awslabs-aws-documentation-mcp-server": {
            "type": "stdio",
            "command": "uvx",
            "args": [
                "awslabs.aws-documentation-mcp-server@latest"
            ],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR"
            },
            "disabled": true,
            "autoApprove": []
        }
    }
}
