{
    "cleanupPeriodDays": 3600,
    "env": {
        "BASH_DEFAULT_TIMEOUT_MS": "3000000",
        "BASH_MAX_TIMEOUT_MS": "3000000",
        "DISABLE_AUTOUPDATER": "1"
    },
    "feedbackSurveyState": {
        "lastShownTime": 2069451551000
    },
    "includeCoAuthoredBy": false,
    "model": "Opus",
    "permissions": {
        "allow": [],
        "deny": [
            "Bash(git push:*)",
            "Bash(git rebase:*)",
            "Bash(git reset:*)",
            "Bash(mv:*)",
            "Bash(rm -rf:*)",
            "Bash(rm:*)",
            "Bash(sudo:*)",
            "Read(**/*key*)",
            "Read(**/*token*)",
            "Read(.env.*)",
            "Read(id_ed25519)",
            "Read(id_rsa)",
            "Write(**/secrets/**)",
            "Write(.env*)"
        ]
    },
    "statusLine": {
        "type": "command",
        "command": "npx ccusage@latest statusline"
    }
