{
  "cleanupPeriodDays": 36000,
  "env": {
    "BASH_DEFAULT_TIMEOUT_MS": "3000000",
    "BASH_MAX_TIMEOUT_MS": "3000000",
    "CLAUDE_CODE_DISABLE_AUTO_MEMORY": "true",
    "CLAUDE_CODE_DISABLE_FEEDBACK_SURVEY": "true",
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "true",
    "CLAUDE_CODE_ENABLE_TASKS": "true",
    "CLAUDE_CODE_ENABLE_TELEMETRY": "false",
    "CLAUDE_CODE_FILE_READ_MAX_OUTPUT_TOKENS": "25000",
    "IS_DEMO": "true"
  },
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "permissions": {
    "deny": [
      "Bash(git -C *)",
      "Bash(git push*)",
      "Bash(git rebase*)",
      "Bash(git reset*)",
      "Bash(rm *)",
      "Bash(sudo *)",
      "Read(**/*key*)",
      "Read(**/*token*)",
      "Read(.env*)",
      "Read(~/.ssh/**)",
      "Write(**/secrets/**)",
      "Write(.env*)"
    ]
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": "compact|resume|clear",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/scripts/sessionstart-reload.sh"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/scripts/precompact-save.sh"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/scripts/statusline.sh"
  }
}
